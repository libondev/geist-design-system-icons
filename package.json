{
  "name": "gdsi",
  "type": "module",
  "version": "2.0.0",
  "description": "Geist design system icons maintained by the community.",
  "author": "Libon <bon.li@outlook.com>",
  "license": "ISC",
  "keywords": [
    "geist design system",
    "geist",
    "icons",
    "svg",
    "vue",
    "react",
    "vanilla"
  ],
  "sideEffects": false,
  "exports": {
    "./svg": {
      "types": "./dist/svg/index.d.ts",
      "default": "./dist/svg/index.js"
    },
    "./vue": {
      "types": "./dist/vue/index.d.ts",
      "default": "./dist/vue/index.js"
    },
    "./react": {
      "types": "./dist/react/index.d.ts",
      "default": "./dist/react/index.js"
    },
    "./resolver": {
      "types": "./dist/resolver.d.ts",
      "default": "./dist/resolver.js"
    },
    "./*": "./dist/*"
  },
  "main": "dist/svg/index.js",
  "types": "./dist/svg/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "sync": "tsx ./scripts/sync.ts",
    "gen": "tsx ./scripts/gen.ts svg vue react",
    "dev:core": "unbuild --stub",
    "build:core": "unbuild",
    "build:lib": "pnpm run build:svg && pnpm run build:vue && pnpm run build:react",
    "build:svg": "pnpm --filter @gdsi/svg build",
    "build:vue": "pnpm --filter @gdsi/vue build",
    "build:react": "pnpm --filter @gdsi/react build",
    "publish": "pnpm run publish:svg && pnpm run publish:vue && pnpm run publish:react",
    "publish:svg": "pnpm --filter @gdsi/svg publish",
    "publish:vue": "pnpm --filter @gdsi/vue publish",
    "publish:react": "pnpm --filter @gdsi/react publish",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@gdsi/react": "workspace:*",
    "@gdsi/svg": "workspace:*",
    "@gdsi/vue": "workspace:*"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.11.2",
    "@types/node": "^22.10.2",
    "cheerio": "^1.0.0",
    "eslint": "^9.16.0",
    "fast-glob": "catalog:",
    "mkdist": "catalog:",
    "tsx": "^4.19.2",
    "typescript": "catalog:",
    "unbuild": "catalog:"
  }
}
