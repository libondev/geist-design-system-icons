{
  "name": "gdsi",
  "type": "module",
  "version": "1.0.3",
  "description": "Geist design system icons maintained by the community.",
  "author": "Libon <bon.li@outlook.com>",
  "license": "ISC",
  "keywords": [
    "geist design system",
    "geist",
    "icons",
    "svg",
    "vue",
    "react",
    "vanilla"
  ],
  "sideEffects": false,
  "exports": {
    "./svg": {
      "types": "./dist/svg/index.d.ts",
      "default": "./dist/svg/index.js"
    },
    "./vue": {
      "types": "./dist/vue/index.d.ts",
      "default": "./dist/vue/index.js"
    },
    "./react": {
      "types": "./dist/react/index.d.ts",
      "default": "./dist/react/index.js"
    },
    "./resolver": {
      "types": "./dist/resolver.d.ts",
      "default": "./dist/resolver.js"
    },
    "./svg/*": {
      "types": "./dist/svg/*.d.ts",
      "default": "./dist/svg/*.js"
    },
    "./vue/*": {
      "types": "./dist/vue/*.d.ts",
      "default": "./dist/vue/*.js"
    },
    "./react/*": {
      "types": "./dist/react/*.d.ts",
      "default": "./dist/react/*.js"
    },
    "./*": "./dist/*"
  },
  "main": "dist/svg/index.js",
  "types": "./dist/svg/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "gen": "tsx ./scripts/gen.ts svg vue react",
    "build": "vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.11.2",
    "@types/node": "^22.10.2",
    "@types/react": "^18.3.1",
    "cheerio": "^1.0.0",
    "eslint": "^9.16.0",
    "fast-glob": "^3.3.2",
    "react": "^18.3.1",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "unplugin-vue-components": "^0.27.5",
    "vite": "^6.0.1",
    "vite-plugin-dts": "^4.3.0",
    "vue": "^3.5.13"
  }
}
